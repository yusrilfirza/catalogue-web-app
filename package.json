{
  "name": "catalogue-web-apps",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "ts-lint-commit-hook": "tsc --noEmit --incremental",
    "format": "prettier --write .",
    "test:watch": "jest --coverage --watch --env=jsdom",
    "test": "jest --coverage --env=jsdom",
    "pre-commit": "yarn format && lint-staged && pretty-quick --staged && yarn ts-lint-commit-hook",
    "ts-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@iconscout/react-unicons": "^2.0.2",
    "@tanstack/react-query": "^5.51.23",
    "@tanstack/react-table": "^8.20.1",
    "axios": "^1.7.4",
    "nanoid": "^5.0.7",
    "next": "^15.0.1",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.2",
    "react-toastify": "^10.0.5",
    "sass": "^1.80.4",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "prettier-quick": "^0.0.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
